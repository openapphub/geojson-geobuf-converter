{
  "name": "geojson-geobuf-converter",
  "version": "1.0.0",
  "description": "A high-performance GeoJSON to GeoBuf compression and decompression tool with web interface",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "security": "npm audit",
    "security:fix": "npm audit fix",
    "build": "echo 'No build step required for Node.js app'",
    "docker:build": "docker build -t geojson-geobuf-converter .",
    "docker:run": "docker run -p 3000:3000 geojson-geobuf-converter",
    "prepare": "husky install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "geobuf": "^3.0.2",
    "pbf": "^3.2.1",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "geojson",
    "geobuf",
    "compression",
    "map",
    "gis",
    "protobuf",
    "nodejs",
    "express",
    "web-interface",
    "file-converter",
    "spatial-data"
  ],
  "author": {
    "name": "OpenAppHub",
    "email": "contact@openapphub.com",
    "url": "https://github.com/openapphub"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openapphub/geojson-geobuf-converter.git"
  },
  "homepage": "https://github.com/openapphub/geojson-geobuf-converter#readme",
  "bugs": {
    "url": "https://github.com/openapphub/geojson-geobuf-converter/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
} 